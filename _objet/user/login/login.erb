<%
=begin
  Vue pour l'identification de l'user
  + après son identification s'il n'a demandé aucune redirectin.
=end
%>
<% if param(:login) %>
  <% User.check_login %>
<% elsif user.identified? %>
  <%= (folder+'login/sous_vues/no_redirection.erb').deserb(user) %>
<% else %>
  <%= (folder+'login/sous_vues/login_form.erb').deserb(user) %>
<% end %>
